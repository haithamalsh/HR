    "prisma:migrate": "prisma migrate dev --name init"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
    ,"pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "prisma": "^5.9.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}

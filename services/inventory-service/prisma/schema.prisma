generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model InventoryItem {
  id        String  @id @default(cuid())
  name      String
  quantity  Int     @default(0)
  minLevel  Int?
  createdAt DateTime @default(now())
}
